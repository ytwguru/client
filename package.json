{
  "name": "client",
  "version": "1.0.0",
  "description": "Static frontend website",
  "main": "webpack.config.js",
  "scripts": {
    "start": "better-npm-run start-dev",
    "lint": "standard",
    "build": "better-npm-run build",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --progress --hot --colors --history-api-fallback --display-error-details ",
      "env": {
        "DEVELOPMENT": true,
        "DEVTOOLS": true,
        "DEPLOY": false
      }
    },
    "build": {
      "command": "webpack -p",
      "env": {
        "DEVELOPMENT": true,
        "DEVTOOLS": true,
        "DEPLOY": false
      }
    },
    "deploy": {
      "command": "webpack -p",
      "env": {
        "DEVELOPMENT": false,
        "DEVTOOLS": false,
        "DEPLOY": true,
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ytwguru/client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ytwguru/client/issues"
  },
  "homepage": "https://github.com/ytwguru/client#readme",
