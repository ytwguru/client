    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.26.0",
    "dotenv": "2.0.0",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
